# Mode debug: True untuk development, False untuk production
DEBUG=True

# Secret key Django (gantilah dengan string acak yang aman)
SECRET_KEY=django-insecure-your-secret-key-here

# Daftar host yang diperbolehkan (pisahkan dengan koma)
ALLOWED_HOSTS=127.0.0.1,localhost

# Konfigurasi database PostgreSQL
DATABASE_NAME=nama_database
DATABASE_USER=username_database
DATABASE_PASSWORD=password_database
DATABASE_HOST=localhost
DATABASE_PORT=5432

# CSRF Trusted Origins jika digunakan (opsional)
CSRF_TRUSTED_ORIGINS=http://localhost,http://127.0.0.1
